{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "plugins": ["jasmine"],
  "extends": "plugin:jasmine/recommended",
  "globals": {
    "withHtml": true,
    "readJSON": true
  },
  "rules": {
    "jasmine/no-spec-dupes": "off",
    "jasmine/new-line-before-expect": "off",
    "jasmine/no-expect-in-setup-teardown": "off"
  }
}
