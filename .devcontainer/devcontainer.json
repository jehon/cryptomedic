{
	"name": "Devcontainer",
	"dockerComposeFile": [
		"../docker-compose.yaml",
		"docker-compose-override.yaml"
	],
	"service": "dev",
	"runServices": [
		"mysql"
	],
	"mounts": [
		"source=/etc/jehon/restricted/cryptomedic.php,target=/etc/jehon/restricted/cryptomedic.php,type=bind",
		"source=/etc/jehon/restricted/cryptomedic.sh,target=/etc/jehon/restricted/cryptomedic.sh,type=bind"
	],
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker": {
			"dockerDashComposeVersion": "v2"
		}
	},
	"extensions": [
		"dbaeumer.vscode-eslint",
		"EditorConfig.EditorConfig",
		"ecmel.vscode-html-css",
		"mkaufman.HTMLHint",
		"nobuhito.printcode",
		"gruntfuggly.todo-tree",
		"meganrogge.template-string-converter",
		"tyriar.luna-paint",
		"adpyke.vscode-sql-formatter",
		"timonwong.shellcheck",
		"ms-azuretools.vscode-docker",
		"rogalmic.bash-debug",
		"xdebug.php-debug",
		"bmewburn.vscode-intelephense-client"
	],
	"forwardPorts": [
		"phpmyadmin:80"
	]
}