    # RewriteCond %{HTTPS} off
    # RewriteCond ! %{ENV:SERVER_ENV} ^dev$
    # # TODO: 301: Permanently Moved
    # RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=302,L]
</ifModule>
