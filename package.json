{
  "name": "CryptomedicApp",
  "private": true,
  "version": "5.0.3",
  "description": "Cryptomedic application for all plateforms",
  "repository": "https://github.com/jehon/CryptomedicApp",
  "license": "MIT",
  "dependencies": {
    "angular": "1.4.x",
    "angular-mocks": "1.4.x",
    "angular-route": "1.4.x"
  },
  "devDependencies": {
    "bower": "1.*",
    "chromedriver": "latest",
    "eslint": "3.*",
    "fs-extra": "latest",
    "geckodriver": "latest",
    "glob": "latest",
    "jasmine-core": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "latest",
    "karma-firefox-launcher": "latest",
    "karma-html-reporter": "latest",
    "karma-jasmine": "latest",
    "karma-junit-reporter": "latest",
    "nightwatch": "latest",
    "nightwatch-html-reporter": "latest",
    "offline-plugin": "latest",
    "recursive-readdir-sync": "latest",
    "selenium-server": "latest",
    "svgo": "latest",
    "sw-precache": "^4.0.0",
    "webcomponentsjs": "latest",
    "website-dependency-tree": "git@github.com:jehon/website-dependency-tree.git"
  },
  "scripts": {
    "build": "npm run cache",
    "test": "./bin/dev-reset.sh && npm run test-api-1.1 && npm run test-api-1.2 && npm run test-unit && npm run test-e2e",
    "test-api-1.1": "cd www/api/v1.1/ && phpunit --coverage-html ../../../target/php1.1 --coverage-xml ../../../target/php1.1 && cd ../../../",
    "test-api-1.2": "cd www/api/v1.2/ && phpunit --coverage-html ../../../target/php1.2 --coverage-xml ../../../target/php1.2 && cd ../../../",
    "test-unit": "karma start --single-run",
    "test-unit-continuously": "karma start",
    "test-e2e": "node e2e-with-server.js",
    "test-e2e-chrome": "nightwatch -e chrome",
    "cache": "cd www && node ../buildCache.js static/index.html",
    "cache-dev": "cd www && node ../../offline-listing/index.js static/index.html",
    "serve" : "php -S localhost:5555 webserver.php"
  }
}
