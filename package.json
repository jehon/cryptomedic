{
  "name": "CryptomedicApp",
  "private": true,
  "version": "5.0.2",
  "description": "Cryptomedic application for all plateforms",
  "repository": "https://github.com/jehon/CryptomedicApp",
  "license": "MIT",
  "dependencies": {
    "angular": "1.4.x",
    "angular-mocks": "1.4.x",
    "angular-route": "1.4.x"
  },
  "devDependencies": {
    "babel-core": "5.*",
    "babel-eslint": "5.*",
    "babel-loader": "5.*",
    "babel-runtime": "5.*",
    "bower": "^1.7.9",
    "chromedriver": "latest",
    "eslint": "3.*",
    "fs-extra": "latest",
    "geckodriver": "^1.1.3",
    "glob": "latest",
    "jasmine-core": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "latest",
    "karma-firefox-launcher": "latest",
    "karma-html-reporter": "latest",
    "karma-jasmine": "latest",
    "karma-junit-reporter": "latest",
    "nightwatch": "latest",
    "nightwatch-html-reporter": "latest",
    "offline-plugin": "latest",
    "phantomjs": "latest",
    "recursive-readdir-sync": "latest",
    "selenium-server": "latest",
    "svgo": "latest",
    "sw-precache": "^4.0.0",
    "webcomponentsjs": "^1.0.2",
    "website-dependency-tree": "git@github.com:jehon/website-dependency-tree.git"
  },
  "scripts": {
    "build": "npm run cache",
    "test": "./bin/dev-reset.sh && npm run test-api && npm run test-unit && npm run test-e2e",
    "test-api": "cd www/api/v1.1/ && phpunit --coverage-html ../../../target/php1.1 --coverage-xml ../../../target/php1.1",
    "test-unit": "karma start --single-run",
    "test-unit-cont": "karma start",
    "test-e2e": "nightwatch -e default,chrome",
    "cache": "cd www && node ../buildCache.js static/index.html",
    "cache-dev": "cd www && node ../../offline-listing/index.js static/index.html"
  }
}
