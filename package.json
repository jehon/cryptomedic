{
  "name": "CryptomedicApp",
  "private": true,
  "version": "4.8.0",
  "description": "Cryptomedic application for all plateforms",
  "repository": "https://github.com/jehon/CryptomedicApp",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.3",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "latest",
    "karma-firefox-launcher": "latest",
    "karma-html-reporter": "latest",
    "karma-jasmine": "latest",
    "karma-junit-reporter": "latest",
    "nightwatch": "latest",
    "nightwatch-html-reporter": "latest",
    "phantomjs": "latest",
    "selenium-server": "latest",
    "selenium-standalone": "latest",
    "shelljs": "latest"
  },
  "x-old": {
    "selenium-wrapper": "latest",
    "selenium-please": "latest",
    "selenium-standalone": "latest",
    "travis-lint": "0.0.5"
  },
  "scripts": {
    "postinstall": "bower install",
    "postupdate": "bower update",
    "test": "gulp test",
    "test:e2e-headless": "xvfb-run --server-args='-screen 0, 1366x768x24' npm run test:e2e",
    "disabled-prestart": "npm install",
    "disabled-pretest": "npm install",
    "disabled-test": "karma start test/karma.conf.js",
    "disabled-start": "http-server -a localhost -p 8000",
    "disabled-test-single-run": "karma start test/karma.conf.js  --single-run",
    "disabled-preupdate-webdriver": "npm install",
    "disabled-update-webdriver": "webdriver-manager update",
    "disabled-preprotractor": "npm run update-webdriver",
    "disabled-protractor": "protractor test/protractor-conf.js",
    "disabled-update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  }
}
