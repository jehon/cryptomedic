#!/usr/bin/env bash

set -o errexit

# shellcheck source=/usr/bin/jh-lib
. jh-lib

export CR_WORKSPACE
CR_WORKSPACE="$(pwd)"

# We are in the running container, use latest sources
envsubst < "$CR_WORKSPACE"/build/setup/cryptomedic/website.conf > /etc/apache2/sites-enabled/website.conf

exec /usr/sbin/apache2ctl -DFOREGROUND -k start
