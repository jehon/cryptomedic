<IfModule mod_expires.c>
	ExpiresActive 	  On
	ExpiresByType	    img/*			              "access plus 1 week"
	ExpiresByType	    css/*			              "access plus 1 week"
	ExpiresByType     js/*			              "access plus 1 week"
	ExpiresByType     text/cache-manifest			A0
	ExpiresDefault 	  A0
</IfModule>

RedirectMatch		404				vendor


########### REDIRECT TO INDEX IF DOES NOT EXISTS ##########
#RewriteCond         %{REQUEST_URI}      !^/api/
#RewriteCond         %{REQUEST_URI}      !^/phpmyadmin
#RewriteCond         %{DOCUMENT_ROOT}/%{REQUEST_URI} !-d
#RewriteCond         %{DOCUMENT_ROOT}/%{REQUEST_URI} !-f
#RewriteRule         /.*                 /opt/biobank/frontend/build/index.html  [L]

########### BACKEND BY VERSIONS ###########################
RewriteRule         ^api/v([0-9]+\.[0-9]+)(/.*)/$     api/v$1$2
RewriteRule         ^api/v([0-9]+\.[0-9]+)            api/v$1/public/index.php
