#!/usr/bin/env bash

set -o errexit
set -o pipefail

# shellcheck source=./cr-lib
. "$(dirname "${BASH_SOURCE[0]}")"/cr-lib

OUTPUT="conf/database/dev/100 base.sql"

mkdir -p "$(dirname "$OUTPUT")"

cr-mysqldump --extended-insert=FALSE cryptomedic >"$OUTPUT"

echo
echo "Backup available at $OUTPUT"
