#!/usr/bin/env bash

set -o errexit
set -o pipefail

# shellcheck source=./cr-lib
. "$(dirname "${BASH_SOURCE[0]}")"/cr-lib

# https://docs.docker.com/compose/env-file/
#
# Bug about newline: see https://issueexplorer.com/issue/docker/compose/8833
#
docker compose --project-directory="$CR_PRJ_DIR" "$@" </dev/null
