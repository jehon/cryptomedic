{
  "plugins": [ "jasmine" ],
  "env": {
    "node": false,
    "browser": true,
    "jasmine": true
  },
  "overrides": [
    {
      "files": [ "*.spec.ts" ],
      "extends" : [ "plugin:jasmine/recommended" ]
    },
    {
      "files": [ "*.ts" ],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}